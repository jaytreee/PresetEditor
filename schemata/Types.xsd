  <xsd:complexType name="DataModelFluenceModel">
    <xsd:all>
      <xsd:element name="DataModelVersion" type="xsd:string" />
      <xsd:element name="CreationTime" type="xsd:dateTime" />
      <xsd:element name="GUID" type="xsd:string" />
      <xsd:element name="Version" type="xsd:decimal" />
      <xsd:element name="isHidden" type="xsd:boolean" />
      <xsd:element name="Hash" type="xsd:string" />
      <xsd:element name="CompatibleDetectorGuids">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="string" type="xsd:string" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ModelName" type="xsd:string" />
      <xsd:element name="ParameterTitle" type="xsd:string" />
      <xsd:element name="ParameterDefaultIndex" type="xsd:int" />
      <xsd:element name="Wavelengths">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="1" maxOccurs="unbounded" name="WavelengthValue" type="xsd:unsignedShort" />
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="ParameterValues">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="CoefficientSet">
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element minOccurs="1" maxOccurs="unbounded" name="CoefficientValue" type="xsd:double" />
                </xsd:sequence>
                <xsd:attribute name="Title" type="xsd:string" use="required" />
                <xsd:attribute name="Description" type="xsd:string" use="optional" />
                <xsd:attribute name="CreationTime" type="xsd:dateTime" use="optional" />
                <xsd:attribute name="unit-modifier" type="xsd:string" use="required" />
                <xsd:attribute name="unit" type="xsd:string" use="required" />
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:all>
  </xsd:complexType>
  <xsd:complexType name="CropLimits">
