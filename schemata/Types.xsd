  <xsd:complexType name="ArrayOfDataModelMspPreset">
    <xsd:sequence>
      <xsd:element name="DataModelMspPreset" type="DataModelMspPreset" maxOccurs="unbounded" minOccurs="0" />
    </xsd:sequence>
  </xsd:complexType>
  <!--this is the original Wavelength Type definition. It is nicknamed strict because the  
  original name Wavelength is "reserved" by old xml versions as explained in definition of Wavelength
  type bellow-->
  <xsd:complexType name="WavelengthStrict">
    <xsd:simpleContent>
      <xsd:extension base="xsd:int">
        <xsd:attribute name="unit" type="xsd:string" use="required" />
        <xsd:attribute name="unit-modifier" type="xsd:string" use="required" />
        <xsd:attribute name="number" type="xsd:int" use="required"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  
  <!--relaxed Wavelength definition, where number attribute is optional. 
  Should be used only in ProjectionMsotFrame.WAVELENGTH element where in 
  very old xml versions it was defined as of Type:Wavelength, while in newer versions it is
  declared as of type PHYSICAL-VALUE making number attribute the only conflict between the 2 types-->
  <xsd:complexType name="Wavelength">
    <xsd:simpleContent>
      <xsd:extension base="xsd:int">
        <xsd:attribute name="unit" type="xsd:string" use="required" />
        <xsd:attribute name="unit-modifier" type="xsd:string" use="required" />
        <xsd:attribute name="number" type="xsd:int" use="optional"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="ArrayOfWavelength">
    <xsd:sequence>
      <xsd:element name="Wavelength" type="WavelengthStrict" maxOccurs="unbounded" minOccurs="0" />
    </xsd:sequence>
  </xsd:complexType>
