      <xsd:element name="ProdigyDataSet" type="xsd:string" minOccurs="0" />
      <xsd:element name="ProdigyDaq" type="xsd:string" minOccurs="0" />
      <xsd:element name="ProdigySubImageIncrement" type="xsd:int" minOccurs="0" />
      <xsd:element name="ProdigyCUDATime" type="xsd:int" minOccurs="0" />
      <xsd:element name="ProdigyInterpolation" type="xsd:int" minOccurs="0" />
