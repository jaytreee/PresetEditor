      <xsd:element name="Threshold" type="xsd:double" minOccurs="0" />
      <xsd:element name="MaxSupportedFrameRate" type="xsd:double" minOccurs="0" />
      <xsd:element name="MipCropLimitsX" type="CropLimits" minOccurs="0" />
      <xsd:element name="MipCropLimitsY" type="CropLimits" minOccurs="0" />
      <xsd:element name="MipCropLimitsZ" type="CropLimits" minOccurs="0" />
