      <xsd:element name="UserSoundTrim" type="xsd:double" minOccurs="0" />
      <xsd:element name="ProdigyFrequency" type="xsd:int" minOccurs="0" />
      <xsd:element name="ProdigyPower" type="xsd:string" minOccurs="0" />
      <xsd:element name="ProdigySV" type="xsd:int" minOccurs="0" />
      <xsd:element name="DirectFilter" type="xsd:boolean" nillable="true" minOccurs="0" />
      <xsd:element name="InvertReconstruction" type="xsd:boolean" minOccurs="0" />
