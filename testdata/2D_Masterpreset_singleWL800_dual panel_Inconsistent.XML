<!-- **************************************************************************************
	 RELEASE 1.2 Single Panel - 2D 800nm Single WL Preset
	 ************************************************************************************** 
	 Last modified: 2019/06/18, Elena Mercep
	 Change the ‘0-Max’ scaling of ultrasound images to ‘Min-Max’ 
	 <UltrasoundMinimumScaling>0</UltrasoundMinimumScaling>
	 <UltrasoundMaximumScaling>1</UltrasoundMaximumScaling>
	 --> 
	 
	 
	 	
<!-- ** ALWAYS change the Identifier upon copying to uniquely identify the Preset.
		Upon Change, tag with your Name and Date of change. 
		This is not shown anywhere in the software, but will appear in the .msot File -->
		<PresetIdentifier>PType212_Master_2D_800nm_v1.2</PresetIdentifier>

<!-- ** COMPATIBLE DETECTOR ** must be matching a detector in settings file -->
		<CompatibleDetectorGUID>PType212</CompatibleDetectorGUID>

<!-- ** Name shown in ViewMSOTc - use a dash ("-") for a new line -->
		<Name>2D, Master - 800nm</Name>
		<PresetType>Factory</PresetType>
		<IsDefaultPreset>false</IsDefaultPreset>
	
<!-- DO NOT CHANGE unless incremented in new software release -->	
		<PresetVersion>3</PresetVersion>
	
	
	
<!-- *********************************************************************
	 PROCESSING SETTINGS 
	 ********************************************************************* -->
	
	
<!-- ** MSP settings
		NOTE: Values returned from MSP will change when changing wavelengths -->
		<MspPresets>
			<DataModelMspPreset>
				<DiscardNegativeValues>true</DiscardNegativeValues>

<!-- ** Spectra for MSP. Use only Names which have spectra in the ProgramData\ViewMSOTc folder
		NOTE: Case sensitive -->
				<UserSelectedSpectra>
				</UserSelectedSpectra>
	
			</DataModelMspPreset>
		</MspPresets>
		<ReconPresets>
			<DataModelReconPreset>
				<SystemReconPresetsVersion>0</SystemReconPresetsVersion>	<!-- DO NOT change unless new software version requires it -->
				<Method>direct</Method>										<!-- Nothing else suppoted at this point -->
				<SystemReconPresets>
					<ReconstructionPreset>
						
<!-- ** RECON PARAMETERS **
		This controls the reconstruction parameters. Change with care
		Note: FoV applies also for Ultrasound -->
						<Resolution>400</Resolution>
						<Projections>512</Projections>
						<RoiLow>0.0200</RoiLow>
				        <RoiHigh>0.0600</RoiHigh>

<!-- ** ULTRASOUND RECON ** -->
						<UltraSoundPixelSize>0.00020</UltraSoundPixelSize>		<!-- Resolution of US image in meters - together with Roi determines number of pixels for US -->
						<ProdigyDataSet>Half</ProdigyDataSet>					<!-- Acquisition of Half or Full dataset (always Half for >128 channels) -->
						<ProdigyDaq>SA</ProdigyDaq>								<!-- Reconstruction Technique, can be SA (recommended) or STA -->
						<ProdigySubImageIncrement>64</ProdigySubImageIncrement> <!-- Number of Elements for Subaperture -->
						<ProdigyInterpolation>2</ProdigyInterpolation>			<!-- Interpolation of projections. Recommended, but reduces frame rate -->
				
<!-- ** FILTERS part 1 ** Parameters for Signal Filtering and Interpolation -->
						<TimeRes>3</TimeRes>			<!-- Interpolation along t, default is 3 -->
						<FilterType>FIR</FilterType>
						<FilterLow>50000</FilterLow>				<!-- High pass cutoff in Hz -->
						<FilterHigh>5500000</FilterHigh>		<!-- Low pass cutoff in Hz -->
			
<!-- ** FoV - 3D ONLY ** Parameters for 3D FoV -->
						<Nz>1</Nz>					<!-- NOTE: Don't remove this for the interim, defaults to 50 -->
						<RoiZlow>0.00</RoiZlow>
						<RoiZhigh>0.01</RoiZhigh>
						
<!-- ** FRAME RATE ** Max Frame Rate - only lower this if dramatically increased number of pixels for reconstruction. Default: 50 -->
						<MaxSupportedFrameRate>50</MaxSupportedFrameRate>
					</ReconstructionPreset>
				</SystemReconPresets>
		
				<DisableLowPass>false</DisableLowPass>	<!-- Switch off High-Pass entirely (default: false). Do not turn on in combination with use of impulse response -->
				<ImpulseResponse>true</ImpulseResponse>	<!-- Use Impulse Response as supplied in Detector Settings -->
		
<!-- ** DEPTH CORRECTION ** -->
				<DepthCorrection>true</DepthCorrection>	<!-- If this is disabled, the Depth correction slider is disabled in the GUI -->
				<BackgroundAbsorption>0.022</BackgroundAbsorption>	<!-- in 1/cm, put to 0 in order to disable, but keep the slider active -->
				<BackgroundOxygenation>70</BackgroundOxygenation>	<!-- in % -->
				<SensitivityMap>false</SensitivityMap>	<!-- Apply Sensitivity Map if specified in detector settings -->
		
<!-- ** SPEED OF SOUND ** Focussing - Add on top of "BaseSpeedOfSound" in Settings -->
				<UserSoundTrim>0</UserSoundTrim>		<!-- Default position of Focus slider -->
				
<!-- ** ULTRASOUND FOCUS ** Ultrasound Frequency and Speed of Sound -->
				<ProdigyFrequency>6</ProdigyFrequency>	<!-- Excitation Frequency, can have integer numbers and should not exceed 3dB bandwidth of transducer -->
				<ProdigySV>1530</ProdigySV>				<!-- Speed of Sound for Prodigy - will not adapt when OA image is focussed -->					
			</DataModelReconPreset>
		</ReconPresets>
	
	
	
<!-- *********************************************************************
	 EXPERIMENT SETTINGS 
	 ********************************************************************* -->
	 
	 
		<ExprerimentPreset>
				
<!-- ** PREVIEW SETTINGS ** -->
			<DisplayAllWavelengths>true</DisplayAllWavelengths>	<!-- Cycling Wavelengths in OA preview, default: true -->
			<USVisible>true</USVisible>
			
<!-- ** AVERAGING ** -->
			<FramesPerWavelength>1</FramesPerWavelength> 			<!-- Number of Frames per Wavelength -->
			<AverageFramesPerWavelength>false</AverageFramesPerWavelength>	<!-- Enable Averaging - Note this will change format data is stored in -->

<!-- ** DEFAULT SINGLE WAVELENGTH PREVIEW WAVELENGTH ** NOTE: Applied if DisplayAllWavelengths is false -->
			<PreferredBackgroundWL>800</PreferredBackgroundWL>
		
<!-- ** TRIGGER RATE ** NOTE: Calibration file must support this, default: 25 -->
			<DefaultLaserTriggerRate>25</DefaultLaserTriggerRate>
		
<!-- ** SELECTIVE FRAME AVERAGING ** -->
		<SelectiveFrameAveraging>
			<REFWAVELENGTH>800</REFWAVELENGTH>		<!-- Wavelength that is compared over time, ideally use something mid-range -->
			<CORRTHRES>0.0000</CORRTHRES>				<!-- Threshold for a sweep to be marked consistent -->
			<MAXAVERAGES>7</MAXAVERAGES>			<!-- Number of sweeps to average, default: 7. Reduce if >10 wavelengths used -->
			<FRAMECORRTHRES>0.88</FRAMECORRTHRES>	<!-- Threshold for a sweep to be marked for averaging with current sweep -->
			<CONFLIMIT>0.25</CONFLIMIT>			    <!-- Threshold for confidence. Currently unused - Ultimately controls when MSP results should be shown -->
		</SelectiveFrameAveraging>
		<MAXPASTSWEEPS>8</MAXPASTSWEEPS>						  
		
<!-- ** WAVELENGTHS ** 
	 NOTE: Changing Wavelengths will change scaling of MSP components -->
			<WavelengthSet>
				<Items>
				  <double>800</double></Items>
			</WavelengthSet>
			
<!-- ** ENABLE MULTIPANEL VIEW ** 
	 NOTE: If this is set to 'true' then the ViewingPresets should be set -->			
			<IsMultipleMspLivePreviewEnabled>false</IsMultipleMspLivePreviewEnabled>
		</ExprerimentPreset>
	
